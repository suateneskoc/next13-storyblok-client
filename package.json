{
  "name": "next13-storyblok-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "node -v && next build",
    "check-format": "prettier --check .",
    "check-types": "tsc --pretty --noEmit",
    "clear": "rm -rf coverage && rm -rf .next && rm -rf .swc && rm -rf .vercel",
    "dev": "next dev",
    "dev:https": "next dev & local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "format": "prettier --write .",
    "lint": "next lint",
    "prepare": "husky install",
    "start": "next start"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.4.0",
    "negotiator": "^0.6.3",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-wrap-balancer": "^1.0.0",
    "tailwind-merge": "^1.13.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.7",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/negotiator": "^0.6.1",
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.3.1",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "lint-staged": "^13.2.3",
    "local-ssl-proxy": "^2.0.5",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1.5",
    "husky": "^8.0.0"
  }
}
